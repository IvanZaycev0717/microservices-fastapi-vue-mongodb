			"name": "/projects - 200 (Lang EN + Sort Date DESC) Copy",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response.code === 200) {\r",
							"    const jsonData = pm.response.json();\r",
							"    \r",
							"    if (jsonData.projects && jsonData.projects.length > 0) {\r",
							"        // –ë–µ—Ä–µ–º –ø–µ—Ä–≤—ã–π –ø—Ä–æ–µ–∫—Ç –∏–∑ —Å–ø–∏—Å–∫–∞\r",
							"        const firstProject = jsonData.projects[0];\r",
							"        const projectId = firstProject.id;\r",
							"        \r",
							"        // –°–æ—Ö—Ä–∞–Ω—è–µ–º project_id –≤ environment\r",
							"        pm.environment.set(\"project_id\", projectId);\r",
							"        \r",
							"        // –õ–æ–≥–∏—Ä—É–µ–º\r",
							"        console.log(\"=== PROJECT ID EXTRACTED ===\");\r",
							"        console.log(\"‚úÖ Project ID:\", projectId);\r",
							"        console.log(\"‚úÖ Project title:\", firstProject.title);\r",
							"        console.log(\"‚úÖ Total projects:\", jsonData.projects.length);\r",
							"        \r",
							"        // –ú–æ–∂–Ω–æ —Ç–∞–∫–∂–µ —Å–æ—Ö—Ä–∞–Ω–∏—Ç—å –Ω–µ—Å–∫–æ–ª—å–∫–æ ID –¥–ª—è —Ç–µ—Å—Ç–æ–≤\r",
							"        if (jsonData.projects.length > 1) {\r",
							"            const secondProjectId = jsonData.projects[1].id;\r",
							"            pm.environment.set(\"project_id_2\", secondProjectId);\r",
							"            console.log(\"‚úÖ Second project ID:\", secondProjectId);\r",
							"        }\r",
							"        \r",
							"    } else {\r",
							"        console.log(\"‚ùå No projects found in response\");\r",
							"    }\r",
							"    \r",
							"} else {\r",
							"    console.log(\"‚ùå Projects request failed with status:\", pm.response.code);\r",
							"}"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
